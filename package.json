  "description": "A system use to render financial services",
  "main": "index.js",
  "scripts": {
    "create": "node dbManager.js createUserTable",
    "drop": "node dbManager.js dropUserTable",
    "clean": "rm -rf lib",
    "build": "npm run drop && npm run create npm run clean && babel src -d lib",
    "start": "node lib/index.js",
    "start-dev": "nodemon --exec node lib/index.js",
